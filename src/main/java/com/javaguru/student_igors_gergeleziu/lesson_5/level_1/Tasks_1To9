class ArrayLength1 {

    public static void main(String[] args) {
        int[] numbers = new int[2];
        System.out.println(numbers[0]);
    }

}
class ArrayLength2 {

    public static void main(String[] args) {
        int[] numbers = new int[2];
        System.out.println(numbers[0]);
        System.out.println(numbers[1]);
    }

}
class ArrayLength3 {

    public static void main(String[] args) {
        int[] numbers = new int[2];
        System.out.println(numbers.length);
    }

}

