class ForLoopDefinition1 {

    public static void main(String[] args) {
        for (int i=0; i < 10; i++) {
            System.out.println(i);
        }
    }

}
class ForLoopDefinition2 {

    public static void main(String[] args) {
        for (int i = 0; i < 10; i++) {
            System.out.println(i);
        }
    }

}
